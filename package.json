{
  "name": "npm-v0-finder",
  "description": "Find 0.x.y npm packages by user.",
  "version": "1.0.0",
  "author": "Luke Karrys",
  "bugs": {
    "url": "https://github.com/lukekarrys/npm-v0-finder/issues"
  },
  "dependencies": {
    "async": "^1.5.0",
    "domify": "^1.3.0",
    "domready": "^1.0.6",
    "underscore": "^1.6.0",
    "xhr": "^2.0.1"
  },
  "devDependencies": {
    "beefy": "^2.1.3",
    "browserify": "^12.0.1",
    "git-directory-deploy": "^1.0.0",
    "jshint": "^2.8.0",
    "moonboots-static": "^2.0.0",
    "precommit-hook": "^3.0.0"
  },
  "homepage": "http://v0.lukecod.es",
  "keywords": [
    "npm",
    "semver"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "lint",
    "validate"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lukekarrys/npm-v0-finder.git"
  },
  "scripts": {
    "build": "moonboots config.js && echo v0.lukecod.es > _dist/CNAME",
    "deploy": "npm run build && git-directory-deploy --directory _dist",
    "lint": "jshint .",
    "start": "beefy app.js",
    "validate": "npm ls"
  }
}
